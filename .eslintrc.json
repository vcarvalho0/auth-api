{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "plugin:jest/recommended",
    "standard"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "no-warning-comments": "warn",
    "react/react-in-jsx-scope": "off",
    "react/prop-types": "off",
    "no-unused-vars": "off",
    "space-before-function-paren": "off"
  }
}
